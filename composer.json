{
    "name": "synersie/magento-module",
    "license": "OSL-3.0",
    "type": "magento-module",
    "description": "Synersie integration for magento",
    "repositories": {
        "installer-magento-core": {
          "type": "git", 
          "url": "https://github.com/AydinHassan/magento-core-composer-installer"
        },
        "magento": {
          "type":"git", 
          "url":"https://github.com/firegento/magento"
        },
        "firegento": {
          "type": "composer",
          "url": "https://packages.firegento.com"
        }
    },
    "require": {
	  "synerise/php-sdk": "dev-master"
	},
	"require-dev": {
      "magento-hackathon/magento-composer-installer": "~3.0",
      "aydin-hassan/magento-core-composer-installer": "~1.2",
      "firegento/magento": "~1.9.2"
    },
    "extra": {
      "magento-root-dir": "src",
      "auto-append-gitignore": true,
      "magento-deploystrategy": "copy",
      "magento-force": true
    }
}